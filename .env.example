# --- General Environment ---
LOG_LEVEL_DEFAULT=debug
LOG_LEVEL_ViemClientManager=info
NODE_ENV=development
IS_OPERATOR_REGISTERED=false
POLLING_INTERVAL_MS=5000
RPC_SERVICE_GIT_BRANCH=master
BLOCK_HISTORY_SIZE=10
# mainnet | testnet | localhost
NETWORK_MODE=testnet
DRY_RUN=false
SIMULATE_TX=false
USE_CHECKPOINTS=false

# --- Git Branches for Remote Configs ---
RPC_SERVICE_GIT_BRANCH=master
DEPLOYMENTS_SERVICE_GIT_BRANCH=master

# --- Localhost Development ---
LOCALHOST_FORK_CHAIN_ID=1
